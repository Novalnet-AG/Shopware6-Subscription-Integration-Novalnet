{% block sw_search_bar_item_cms_page %}
    {% parent %}

    <router-link v-else-if="type === 'novalnet_subscription'"
                 v-bind:to="{ name: 'novalnet.subsctiption.orders.detail.base', params: { id: item.id } }"
                 ref="routerLink"
                 class="sw-search-bar-item__link">
        {% block sw_search_bar_item_novalnet_bar_label %}
            <span class="sw-search-bar-item__label">
                <sw-highlight-text v-bind:searchTerm="searchTerm"
                                   v-bind:text="item.name">
                </sw-highlight-text>
            </span>
        {% endblock %}
    </router-link>
{% endblock %}
